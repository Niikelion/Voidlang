class Pos
{
    priv f32 x,y;
    pub:
    f32 magSqr() -> x*x + y*y
    f32 mag() -> sqrt(magSqr())

    f32 dist(Pos other) -> Pos{ x - other.x, y - other.y }.magSqr()
}

class Optional<T>
{
    priv T t
    priv bool present
}

var optI = Optional<int32>{}

